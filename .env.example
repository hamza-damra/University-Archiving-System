# ============================================
# ENVIRONMENT VARIABLES - ARCHIVE SYSTEM
# ============================================
# Copy this file to .env and update the values
# NEVER commit .env to version control!
# ============================================

# MySQL Configuration
MYSQL_ROOT_PASSWORD=your_secure_root_password
MYSQL_DATABASE=archive_system
MYSQL_USER=archive_user
MYSQL_PASSWORD=your_secure_password
MYSQL_PORT=3307

# Application Configuration
APP_PORT=8080

# JWT Secret (minimum 64 characters for security)
JWT_SECRET=your_super_secret_jwt_key_that_should_be_at_least_64_characters_long

# phpMyAdmin Port (optional, only with --profile tools)
PHPMYADMIN_PORT=8081
